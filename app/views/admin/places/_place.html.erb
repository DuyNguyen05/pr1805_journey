<tr>
  <td><%= place.id %></td>
  <td><%= place.name%></td>
  <td></td>
  <td><%= image_tag "lt-03", class: "img-fluid-1" %></td>
  <td>
    <%= link_to "Edit", edit_admin_place_path(place), class: "btn btn-warning" %>
    <% if place.status == "archived" %>
      <%= link_to "Remove", admin_place_path(place), method: :delete, class: "btn btn-danger" %>
    <% else %>
      <%= link_to "Restore", admin_place_path(place), method: :delete, class: "btn btn-secondary" %>
    <% end %>
  </td>
</tr>
